{
  "name": "nodeserver",
  "type": "module",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npx dotenv -e .env.development nodemon app.js",
    "server": "npx dotenv -e .env.development node app.js",
    "prod": "npx dotenv -e .env.production node app.js",
    "docker": "npx dotenv -e .env.docker-development -- npx prisma migrate dev && npx dotenv -e .env.docker-development -- node app.js",
    "migrate": "dotenv -e .env.development -- npx prisma migrate dev --name migrate1",
    "migrate:reset": "dotenv -e .env.development -- npx npx prisma migrate reset",
    "prisma": "dotenv -e .env.development -- npx npx prisma studio"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.9.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.3.0",
    "morgan": "^1.10.0",
    "mysql2": "^3.6.5",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "chai": "^5.0.0",
    "dotenv-cli": "^7.3.0",
    "mocha": "^10.2.0",
    "prisma": "^5.9.0"
  }
}
